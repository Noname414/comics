{
  "version": 2,
  "builds": [
    {
      "src": "docs/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google-api-key"
  },
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  }
} 